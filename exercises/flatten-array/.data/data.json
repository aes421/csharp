{
  "exercise": "flatten-array",
  "version": "1.2.0",
  "cases": [
    {
      "uuid": "a8e329ea-2139-43bc-8aa3-01e1b2424a4d",
      "description": "no nesting",
      "property": "flatten",
      "input": {
        "array": [0, 1, 2]
      },
      "expected": [0, 1, 2]
    },
    {
      "uuid": "9c0f1904-4966-4e1d-be4a-09338ec0af01",
      "description": "flattens array with just integers present",
      "property": "flatten",
      "input": {
        "array": [1, [2, 3, 4, 5, 6, 7], 8]
      },
      "expected": [1, 2, 3, 4, 5, 6, 7, 8]
    },
    {
      "uuid": "fe6bab89-8d0a-4317-84c0-ba06c8602809",
      "description": "5 level nesting",
      "property": "flatten",
      "input": {
        "array": [0, 2, [[2, 3], 8, 100, 4, [[[50]]]], -2]
      },
      "expected": [0, 2, 2, 3, 8, 100, 4, 50, -2]
    },
    {
      "uuid": "913be088-838c-4608-a41e-4ef27f14334d",
      "description": "6 level nesting",
      "property": "flatten",
      "input": {
        "array": [1, [2, [[3]], [4, [[5]]], 6, 7], 8]
      },
      "expected": [1, 2, 3, 4, 5, 6, 7, 8]
    },
    {
      "uuid": "055038b7-f78d-4a55-9bbb-32461483c27f",
      "description": "6 level nest list with null values",
      "property": "flatten",
      "input": {
        "array": [0, 2, [[2, 3], 8, [[100]], null, [[null]]], -2]
      },
      "expected": [0, 2, 2, 3, 8, 100, -2]
    },
    {
      "uuid": "983d5c73-1809-406a-92d2-efb9b3201dba",
      "description": "all values in nested list are null",
      "property": "flatten",
      "input": {
        "array": [null, [[[null]]], null, null, [[null, null], null], null]
      },
      "expected": []
    }
  ]
}
