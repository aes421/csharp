  "version": "1.2.1",
  "comments": ["Grid rows and columns are 1-indexed."],
  "cases": [
    {
      "uuid": "5a1087e8-6d1b-4d7d-be67-ebec6175f0f8",
      "description": "Should accept an initial game grid and a target search word",
      "property": "search",
      "input": {
        "grid": ["jefblpepre"],
        "wordsToSearchFor": ["clojure"]
      },
      "expected": {
        "clojure": null
      }
    },
    {
      "uuid": "22c2f228-efdb-492c-adfe-93e247adc763",
      "description": "Should locate one word written left to right",
      "property": "search",
      "input": {
        "grid": ["clojurermt"],
        "wordsToSearchFor": ["clojure"]
      },
      "expected": {
        "clojure": {
          "start": {
            "column": 1,
            "row": 1
          },
          "end": {
            "column": 7,
            "row": 1
          }
        }
      }
    },
