{
  "exercise": "phone-number",
  "version": "1.7.0",
  "cases": [
    {
      "description": "Cleanup user-entered phone numbers",
      "comments": [
        " Returns the cleaned phone number if given number is valid, ",
        " else returns error object. Note that number is not formatted,",
        " just a 10-digit number is returned.                        "
      ],
      "cases": [
        {
          "uuid": "b2a832a8-41bb-4900-b80e-5cd504ea8059",
          "description": "cleans the number",
          "property": "clean",
          "input": {
            "phrase": "(223) 456-7890"
          },
          "expected": "2234567890"
        },
        {
          "uuid": "21b3c104-9da5-47a1-8ccd-e95cc2ffab11",
          "description": "cleans numbers with dots",
          "property": "clean",
          "input": {
            "phrase": "223.456.7890"
          },
          "expected": "2234567890"
        },
        {
          "uuid": "dc81a836-50e5-446a-92bc-e75b588af2e7",
          "description": "cleans numbers with multiple spaces",
          "property": "clean",
          "input": {
            "phrase": "223 456   7890   "
          },
          "expected": "2234567890"
        },
        {
          "uuid": "541609c2-e848-4147-959f-d4d9659e0c58",
          "description": "invalid when 9 digits",
          "property": "clean",
          "input": {
            "phrase": "123456789"
          },
          "expected": {
            "error": "incorrect number of digits"
          }
        },
        {
          "uuid": "3dea0f44-f91e-4fa7-bc78-20ce895e5b0b",
          "description": "invalid when 11 digits does not start with a 1",
          "property": "clean",
          "input": {
            "phrase": "22234567890"
          },
          "expected": {
            "error": "11 digits must start with 1"
          }
        },
        {
          "uuid": "2643d9ea-89ba-4921-b4ff-2724908e3ddf",
          "description": "valid when 11 digits and starting with 1",
          "property": "clean",
          "input": {
            "phrase": "12234567890"
          },
          "expected": "2234567890"
        },
        {
          "uuid": "45ed1784-bb5f-48e0-8aa4-7edf6711f027",
          "description": "valid when 11 digits and starting with 1 even with punctuation",
          "property": "clean",
          "input": {
            "phrase": "+1 (223) 456-7890"
          },
          "expected": "2234567890"
        },
        {
          "uuid": "de6343d8-2f84-4d50-98e8-b32ba95c499c",
          "description": "invalid when more than 11 digits",
          "property": "clean",
          "input": {
            "phrase": "321234567890"
          },
          "expected": {
            "error": "more than 11 digits"
          }
        },
        {
          "uuid": "554e5301-32d7-400e-8c40-f0fe58b258f1",
          "description": "invalid with letters",
          "property": "clean",
          "input": {
            "phrase": "123-abc-7890"
          },
          "expected": {
            "error": "letters not permitted"
          }
        },
        {
          "uuid": "7f43d824-f59d-4bb8-91ae-a9e8e8188289",
          "description": "invalid with punctuations",
          "property": "clean",
          "input": {
            "phrase": "123-@:!-7890"
          },
          "expected": {
            "error": "punctuations not permitted"
          }
        },
        {
          "uuid": "e37943b1-ad03-4dce-8012-8030fd8d63d4",
          "description": "invalid if area code starts with 0",
          "property": "clean",
          "input": {
            "phrase": "(023) 456-7890"
          },
          "expected": {
            "error": "area code cannot start with zero"
          }
        },
        {
          "uuid": "36de2b0b-1a04-4e2e-88f3-6f2c0740e52a",
          "description": "invalid if area code starts with 1",
          "property": "clean",
          "input": {
            "phrase": "(123) 456-7890"
          },
          "expected": {
            "error": "area code cannot start with one"
          }
        },
        {
          "uuid": "8e4dfd67-d9e2-4c29-931a-54a33f5f7af9",
          "description": "invalid if exchange code starts with 0",
          "property": "clean",
          "input": {
            "phrase": "(223) 056-7890"
          },
          "expected": {
            "error": "exchange code cannot start with zero"
          }
        },
        {
          "uuid": "4f1ef5bb-5fd3-4f4c-8e15-2c96c49b3ab2",
          "description": "invalid if exchange code starts with 1",
          "property": "clean",
          "input": {
            "phrase": "(223) 156-7890"
          },
          "expected": {
            "error": "exchange code cannot start with one"
          }
        },
        {
          "uuid": "9356f970-9f50-4fe2-bf82-3d48145d5b31",
          "description": "invalid if area code starts with 0 on valid 11-digit number",
          "property": "clean",
          "input": {
            "phrase": "1 (023) 456-7890"
          },
          "expected": {
            "error": "area code cannot start with zero"
          }
        },
        {
          "uuid": "569df8a1-a94e-4b27-851f-f740651ae854",
          "description": "invalid if area code starts with 1 on valid 11-digit number",
          "property": "clean",
          "input": {
            "phrase": "1 (123) 456-7890"
          },
          "expected": {
            "error": "area code cannot start with one"
          }
        },
        {
          "uuid": "c63c8d92-6f33-4a95-be77-8ce907f3fb7a",
          "description": "invalid if exchange code starts with 0 on valid 11-digit number",
          "property": "clean",
          "input": {
            "phrase": "1 (223) 056-7890"
          },
          "expected": {
            "error": "exchange code cannot start with zero"
          }
        },
        {
          "uuid": "bc165bf5-dce3-4e60-b213-76cb51a32297",
          "description": "invalid if exchange code starts with 1 on valid 11-digit number",
          "property": "clean",
          "input": {
            "phrase": "1 (223) 156-7890"
          },
          "expected": {
            "error": "exchange code cannot start with one"
          }
        }
      ]
    }
  ]
}
