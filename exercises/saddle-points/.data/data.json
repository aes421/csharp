{
  "exercise": "saddle-points",
  "version": "1.5.0",
  "comments": ["Matrix rows and columns are 1-indexed."],
  "cases": [
    {
      "uuid": "cbdb090a-f3ed-4e0a-9efa-f0610b4dc49a",
      "description": "Can identify single saddle point",
      "comments": ["This is the README example."],
      "property": "saddlePoints",
      "input": {
        "matrix": [
          [9, 8, 7],
          [5, 3, 2],
          [6, 6, 7]
        ]
      },
      "expected": [
        {
          "row": 2,
          "column": 1
        }
      ]
    },
    {
      "uuid": "a4346c84-0bcf-419e-ae68-998716fa6ead",
      "description": "Can identify that empty matrix has no saddle points",
      "property": "saddlePoints",
      "input": {
        "matrix": [[]]
      },
      "expected": []
    },
    {
      "uuid": "e58962fe-c56a-47f1-85dc-f5aa1094bd06",
      "description": "Can identify lack of saddle points when there are none",
      "property": "saddlePoints",
      "input": {
        "matrix": [
          [1, 2, 3],
          [3, 1, 2],
          [2, 3, 1]
        ]
      },
      "expected": []
    },
    {
      "uuid": "c4c0cd9d-fffe-4fc7-9326-8e5ea89690b6",
      "description": "Can identify multiple saddle points in a column",
      "property": "saddlePoints",
      "input": {
        "matrix": [
          [4, 5, 4],
          [3, 5, 5],
          [1, 5, 4]
        ]
      },
      "expected": [
        {
          "row": 1,
          "column": 2
        },
        {
          "row": 2,
          "column": 2
        },
        {
          "row": 3,
          "column": 2
        }
      ]
    },
    {
      "uuid": "bb76939f-62d3-4ac1-a801-d9dbac299f73",
      "description": "Can identify multiple saddle points in a row",
      "property": "saddlePoints",
      "input": {
        "matrix": [
          [6, 7, 8],
          [5, 5, 5],
          [7, 5, 6]
        ]
      },
      "expected": [
        {
          "row": 2,
          "column": 1
        },
        {
          "row": 2,
          "column": 2
        },
        {
          "row": 2,
          "column": 3
        }
      ]
    },
    {
      "uuid": "fdcb2a07-0958-4e76-98bf-e43e09a366fa",
      "description": "Can identify saddle point in bottom right corner",
      "comments": [
        "This is a permutation of the README matrix designed to test",
        "off-by-one errors."
      ],
      "property": "saddlePoints",
      "input": {
        "matrix": [
          [8, 7, 9],
          [6, 7, 6],
          [3, 2, 5]
        ]
      },
      "expected": [
        {
          "row": 3,
          "column": 3
        }
      ]
    },
    {
      "uuid": "2d180d99-18d5-42dc-bb44-cad46747837c",
      "description": "Can identify saddle points in a non square matrix",
      "property": "saddlePoints",
      "input": {
        "matrix": [
          [3, 1, 3],
          [3, 2, 4]
        ]
      },
      "expected": [
        {
          "row": 1,
          "column": 3
        },
        {
          "row": 1,
          "column": 1
        }
      ]
    },
    {
      "uuid": "244bc8d7-7f75-49b1-a62c-d95f2a65e67e",
      "description": "Can identify that saddle points in a single column matrix are those with the minimum value",
      "property": "saddlePoints",
      "input": {
        "matrix": [[2], [1], [4], [1]]
      },
      "expected": [
        {
          "row": 2,
          "column": 1
        },
        {
          "row": 4,
          "column": 1
        }
      ]
    },
    {
      "uuid": "92c63f3c-491e-4930-a98b-d69807ef57af",
      "description": "Can identify that saddle points in a single row matrix are those with the maximum value",
      "property": "saddlePoints",
      "input": {
        "matrix": [[2, 5, 3, 5]]
      },
      "expected": [
        {
          "row": 1,
          "column": 2
        },
        {
          "row": 1,
          "column": 4
        }
      ]
    }
  ]
}
