{
  "exercise": "sgf-parsing",
  "version": "1.2.0",
  "cases": [
    {
      "uuid": "32e13212-0c8a-4f6b-af32-28b758bead96",
      "description": "empty input",
      "property": "parse",
      "input": {
        "encoded": ""
      },
      "expected": {
        "error": "tree missing"
      }
    },
    {
      "uuid": "42bbec03-4d04-46db-9879-1abe8a6e6f12",
      "description": "tree with no nodes",
      "property": "parse",
      "input": {
        "encoded": "()"
      },
      "expected": {
        "error": "tree with no nodes"
      }
    },
    {
      "uuid": "8da03ebf-0506-41fd-b9b3-2f01f973878c",
      "description": "node without tree",
      "property": "parse",
      "input": {
        "encoded": ";"
      },
      "expected": {
        "error": "tree missing"
      }
    },
    {
      "uuid": "a024d2ad-1d30-4640-a8cc-feccc56ee2a0",
      "description": "node without properties",
      "property": "parse",
      "input": {
        "encoded": "(;)"
      },
      "expected": {
        "properties": {},
        "children": []
      }
    },
    {
      "uuid": "18f40566-2f2b-4165-9d2d-d2dc11079a0a",
      "description": "single node tree",
      "property": "parse",
      "input": {
        "encoded": "(;A[B])"
      },
      "expected": {
        "properties": {
          "A": ["B"]
        },
        "children": []
      }
    },
    {
      "uuid": "9433adea-844e-4d9f-9791-c6fdbf8517f4",
      "description": "multiple properties",
      "property": "parse",
      "input": {
        "encoded": "(;A[b]C[d])"
      },
      "expected": {
        "properties": {
          "A": ["b"],
          "C": ["d"]
        },
        "children": []
      }
    },
    {
      "uuid": "5396858e-1c9f-4adc-8ca8-49930e0066a4",
      "description": "properties without delimiter",
      "property": "parse",
      "input": {
        "encoded": "(;A)"
      },
      "expected": {
        "error": "properties without delimiter"
      }
    },
    {
      "uuid": "38aadeec-43cb-47b4-892e-b52d4aad7cc0",
      "description": "all lowercase property",
      "property": "parse",
      "input": {
        "encoded": "(;a[b])"
      },
      "expected": {
        "error": "property must be in uppercase"
      }
    },
    {
      "uuid": "4b0ef788-5040-434f-92fb-60b73f02e2c9",
      "description": "upper and lowercase property",
      "property": "parse",
      "input": {
        "encoded": "(;Aa[b])"
      },
      "expected": {
        "error": "property must be in uppercase"
      }
    },
    {
      "uuid": "1a84fe82-9c0c-45ce-8e40-19f4f40a0ad3",
      "description": "two nodes",
      "property": "parse",
      "input": {
        "encoded": "(;A[B];B[C])"
      },
      "expected": {
        "properties": {
          "A": ["B"]
        },
        "children": [
          {
            "properties": {
              "B": ["C"]
            },
            "children": []
          }
        ]
      }
    },
    {
      "uuid": "1a763614-40ef-4e7c-86e6-5b1a30ed96fa",
      "description": "two child trees",
      "property": "parse",
      "input": {
        "encoded": "(;A[B](;B[C])(;C[D]))"
      },
      "expected": {
        "properties": {
          "A": ["B"]
        },
        "children": [
          {
            "properties": {
              "B": ["C"]
            },
            "children": []
          },
          {
            "properties": {
              "C": ["D"]
            },
            "children": []
          }
        ]
      }
    },
    {
      "uuid": "5bdff4d2-d51c-4135-a865-844de44f2003",
      "description": "multiple property values",
      "property": "parse",
      "input": {
        "encoded": "(;A[b][c][d])"
      },
      "expected": {
        "properties": {
          "A": ["b", "c", "d"]
        },
        "children": []
      }
    },
    {
      "uuid": "37007725-77a5-474d-8f3a-eec93d65d0a5",
      "description": "escaped property",
      "property": "parse",
      "input": {
        "encoded": "(;A[\\]b\\nc\\nd\\t\\te \\n\\]])"
      },
      "expected": {
        "properties": {
          "A": ["]b\\nc\\nd  e \\n]"]
        },
        "children": []
      }
    }
  ]
}
