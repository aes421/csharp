{
  "exercise": "matrix",
  "version": "1.3.0",
  "cases": [
    {
      "uuid": "ef85a3bf-907d-4b90-97bf-bbe3eacf4957",
      "description": "extract row from one number matrix",
      "property": "row",
      "input": {
        "string": "1",
        "index": 1
      },
      "expected": [1]
    },
    {
      "uuid": "641f076f-420b-4763-903f-1a83312693b7",
      "description": "can extract row",
      "property": "row",
      "input": {
        "string": "1 2\n3 4",
        "index": 2
      },
      "expected": [3, 4]
    },
    {
      "uuid": "bc357cc9-5f39-4fdd-b4b4-773cec46e799",
      "description": "extract row where numbers have different widths",
      "property": "row",
      "input": {
        "string": "1 2\n10 20",
        "index": 2
      },
      "expected": [10, 20]
    },
    {
      "uuid": "09da45b5-5167-47f1-958b-5bbb68266c18",
      "description": "can extract row from non-square matrix with no corresponding column",
      "property": "row",
      "input": {
        "string": "1 2 3\n4 5 6\n7 8 9\n8 7 6",
        "index": 4
      },
      "expected": [8, 7, 6]
    },
    {
      "uuid": "e84f1a4c-d9f8-4c99-830e-cf2c74d864af",
      "description": "extract column from one number matrix",
      "property": "column",
      "input": {
        "string": "1",
        "index": 1
      },
      "expected": [1]
    },
    {
      "uuid": "363dc2ef-0a35-4cb3-ac2d-8eeb402a794b",
      "description": "can extract column",
      "property": "column",
      "input": {
        "string": "1 2 3\n4 5 6\n7 8 9",
        "index": 3
      },
      "expected": [3, 6, 9]
    },
    {
      "uuid": "bec0fbf3-6588-4656-b145-ddaa8fa85eb3",
      "description": "can extract column from non-square matrix with no corresponding row",
      "property": "column",
      "input": {
        "string": "1 2 3 4\n5 6 7 8\n9 8 7 6",
        "index": 4
      },
      "expected": [4, 8, 6]
    },
    {
      "uuid": "10c23fff-4409-466e-85aa-a1288d7daf9b",
      "description": "extract column where numbers have different widths",
      "property": "column",
      "input": {
        "string": "89 1903 3\n18 3 1\n9 4 800",
        "index": 2
      },
      "expected": [1903, 3, 4]
    }
  ]
}
